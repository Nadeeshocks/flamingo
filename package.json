{
  "name": "flamingo",
  "version": "1.0.0",
  "description": "Flamingo for woman",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/scss/index.scss src/css/index.css",
    "watch-css": "npm run build-css &  node-sass-chokidar src/scss/index.scss src/css/index.css --watch",
    "mock:api": "json-server --watch src/js/APIs/data_store.json --port 4000",
    "start": "webpack-dev-server --open --hot --config webpack.dev.js",
    "app": "npm-run-all -p mock:api watch-css start",
    "build": "webpack -p --config webpack.prod.js"
  },
  "author": "Nadia Iqbal",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "immer": "^8.0.1",
    "lodash": "^4.17.11",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "4.3.1",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "chokidar-cli": "^1.2.2",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "json-server": "^0.14.2",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.5"
  }
}
