{
  "name": "flamingo",
  "version": "1.0.0",
  "description": "Flamingo for woman",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "build-task:scss-compile": "node-sass-chokidar --source-map true src/scss/index.scss -o src/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'src/scss/*.scss' -c 'npm run sass:build'",
    "dev": "npm-run-all -p sass:*",
    "start": "webpack-dev-server --open --hot --config webpack.dev.js ",
    "app" :"npm-run-all -p dev start",
    "build": "webpack -p --config webpack.prod.js"
  },
  "author": "Nadia Iqbal",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.11",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "4.3.1",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.1",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.1",
    "node-sass-chokidar": "^1.3.4",
    "postcss-cli": "^6.1.2",
    "chokidar-cli": "^1.2.2",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^0.23.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.5"
  }
}
